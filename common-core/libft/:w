#include <stdio.h>

char **ft_split(char const *s, char c);

int	main(int argc, char **argv)
{
	if (argc != 3)
		printf("not enough args");
	else
	{
		char c = argv[2][0];
		char *string = argv[1];
		ft_split(string, c);
	}
}

char **ft_split(char const *s, char c)
{
	int	words;
	int	letters;
	char **arr;

	words = count_words(s, c);
	return (arr);
}
